package com.psl.client;

import java.sql.Connection;
import java.sql.Date;
import java.sql.DriverManager;
import java.sql.SQLException;

import com.psl.util.BankAccount;

public class Client {

	static{
		try {
			Class.forName("com.mysql.jdbc.Driver");
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
	
	public static void main(String[] args) {
		Connection conn=null;
		try {
			BankAccount ac1=new BankAccount("2345", 12345, "tony stark", Date.valueOf("2019-01-21"));
			BankAccount ac2=new BankAccount("8887", 1000005, "aman gupta", Date.valueOf("2015-06-03"));
			
		conn=	DriverManager.getConnection("jdbc:mysql://localhost:3306/cms", "root", "root");
		} catch (SQLException e) {
		e.printStackTrace();
		}finally{
			if(conn!=null){
				try {
					conn.close();
				} catch (SQLException e2) {
				e2.printStackTrace();
				}
			}
		}
		
	}
	
}
