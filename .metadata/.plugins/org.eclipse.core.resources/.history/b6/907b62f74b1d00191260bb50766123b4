package com.psl.client;

import java.io.FileReader;
import java.io.IOException;
import java.util.Properties;

public class Main {
public static void main(String[] args) {
	Properties p=new Properties();
	try {
		p.load(new FileReader("src/configuration.properties"));
		for(Object i:p.keySet()){
		
		System.out.println(i+" : "+p.getProperty("OS"));
		System.out.println(i+" : "+p.getProperty("Processor"));
		System.out.println(i+" : "+p.getProperty("Storage"));
		System.out.println(i+" : "+p.getProperty("RAM"));}
	} catch (IOException e) {
	e.printStackTrace();
	}
	
}
}
